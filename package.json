  "type": "module",
  "main": "server.js",
  "scripts": {
    "buildextern": "cd perf/extern/opencv-4.13.0/build && cmake -G \"Unix Makefiles\" -DBUILD_LIST=core,imgproc,features2d,flann,img_hash,imgcodecs -DOPENCV_EXTRA_MODULES_PATH=../opencv_contrib-4.13.0/modules -DBUILD_WITH_STATIC_CRT=OFF -DBUILD_SHARED_LIBS=OFF .. && make",
    "e2eServer": "npx @dotenvx/dotenvx run -f build/.env.test -- node server.js",
    "e2e": "npx @dotenvx/dotenvx run -f build/.env.test -- node e2etest/test.js",
    "phashtest": "npx @dotenvx/dotenvx run -f build/.env.phashtest -- node test-dup/test-dup.js",
    "phashgd": "npx @dotenvx/dotenvx run -f build/.env.phashtest -- node test-dup/test-gradient-descent-params.js",
    "dev": "npx @dotenvx/dotenvx run -f build/.env.dev -- node server.js",
    "buildcpp": "cd perf/perftags && make && cd ../perfimg && make",
    "build": "node build/pre-build.js && npx rsbuild --config rsbuild.config.mjs build && node build/post-build.js"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@types/express": "^5.0.2",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^12.6.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "knuth-shuffle-seeded": "^1.0.6",
    "multer": "^2.0.1",
    "node-stream-zip": "^1.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "selenium-webdriver": "^4.38.0",
    "serve-static": "^2.2.0",
    "sharp": "^0.34.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@rsbuild/core": "^1.3.15",
    "@rsbuild/plugin-react": "^1.3.0"
  }
}
